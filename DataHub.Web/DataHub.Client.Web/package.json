{
  "name": "sdcoe-data-hub",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@azure/msal-browser": "^2.12.0",
    "@azure/msal-react": "^1.0.0-beta.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.32",
    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "@fortawesome/react-fontawesome": "^0.1.11",
    "@types/react-show-more": "^2.0.2",
    "bootstrap": "^4.5.3",
    "jquery": "^3.5.1",
    "mobx": "^6.0.4",
    "mobx-react": "^7.0.5",
    "mobx-react-lite": "^3.1.6",
    "mobx-state-router": "6.0.0-beta.1",
    "mobx-utils": "^5.6.2",
    "moment": "^2.29.1",
    "popper.js": "^1.16.1",
    "react": "^16.14.0",
    "react-bootstrap": "^1.3.0",
    "react-dom": "^16.14.0",
    "react-google-button": "^0.7.1",
    "react-hook-form": "^6.9.4",
    "react-html-parser": "^2.0.2",
    "react-icons": "^4.2.0",
    "react-scripts": "3.4.3",
    "react-show-more": "^2.0.0",
    "react-storage-hooks": "^4.0.1",
    "react-toastify": "^7.0.3",
    "typescript": "~4.0.3",
    "wretch": "^1.7.4"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-typescript": "^7.12.7",
    "@testing-library/dom": "^7.29.4",
    "@testing-library/react": "^11.2.3",
    "@testing-library/user-event": "^12.6.0",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.21",
    "@types/react": "^17.0.0",
    "@types/react-bootstrap": "^0.32.25",
    "@types/react-dom": "^17.0.0",
    "@types/react-html-parser": "^2.0.1",
    "babel-plugin-jsx-control-statements": "^4.1.0",
    "eslint-plugin-jsx-control-statements": "^2.2.1",
    "jest": "^26.6.3",
    "miragejs": "^0.1.41",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "jest",
    "jest:watchAll": "yarn test --watchAll --coverage",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "plugins": [
      "jsx-control-statements"
    ],
    "extends": [
      "react-app",
      "plugin:jsx-control-statements/recommended"
    ],
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "-vs-binding": {
    "BeforeBuild": [
      "build"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "**/*.ts",
      "**/*.tsx",
      "**/*.js",
      "**/*.jsx"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/node_modules"
    ],
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "clover"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "transform": {
      ".ts": "ts-jest"
    },
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "verbose": true
  }
}
